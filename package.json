{
  "name": "bar-board",
  "productName": "bar-board",
  "version": "v1.5.1",
  "description": "Beheer de bar van je vereniging",
  "main": "src/index.js",
  "scripts": {
    "build" : "npm build .",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "electron:publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "sass": "sass --watch _sass/main.scss:src/static/css/main.min.css --style compressed --sourcemap=none",
    "serve": "cd src && browser-sync start -s -f . --no-notify --host $LOCAL_IP --port 9000"
  },
  "keywords": [],
  "author": {
    "name": "gdm-lenndery",
    "email": "32176355+gdm-1718-lenndery@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "bar_board"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "dexie": "^2.0.4",
    "push.js": "^1.0.12"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "electron": "7.1.3"
  }
}
